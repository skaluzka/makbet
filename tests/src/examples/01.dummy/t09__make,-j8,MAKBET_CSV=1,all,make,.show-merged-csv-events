#!/usr/bin/env bash
# shellcheck disable=SC2002,SC2164
set -u
set -x


readonly output_file="${1}"
readonly expected_file="${2}"
pushd "${MAKBET_PATH}/examples/01.dummy/" > /dev/null
make makbet-clean
make -j8 MAKBET_CSV=1 all

make .show-merged-csv-events \
    > "${output_file}"

diff -u -s \
    <( \
        cat "${output_file}" \
        | cut -d ";" -f1-3,5-6 \
        | nl -b a \
        | sed '26d' \
    ) \
    <( \
        cat "${expected_file}" \
        | cut -d ";" -f1-3,5-6 \
        | nl -b a \
    )
readonly __rc=$?
popd > /dev/null
exit ${__rc}


# End of file
