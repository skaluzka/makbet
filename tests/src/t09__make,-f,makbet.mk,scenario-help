#!/usr/bin/env bash
# shellcheck disable=SC2002,SC2164
set -u
set -x


readonly output_file="${1}"
readonly expected_file="${2}"
pushd "${MAKBET_PATH}" > /dev/null

make -f "makbet.mk" scenario-help \
    > "${output_file}"

diff -u -s \
    <( \
        cat "${output_file}" \
    ) \
    <( \
        cat "${expected_file}" \
    )
readonly __rc=$?
popd > /dev/null
exit ${__rc}


# End of file
